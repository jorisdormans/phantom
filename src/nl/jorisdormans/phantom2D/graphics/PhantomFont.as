						}
					} else {
						if (font[c] != null) {
							w += widths[c] * scale + characterSpacing * scale * characterHeight;
						}
					}
				}
			}
			if (w>0) w-=characterSpacing * scale * characterHeight;
			
			return w;
		}
		
		public static function drawTextLine(s:String, graphics:Graphics, x:Number, y:Number , size:Number, align:String):int {
			var drawWidth:Number = lineWidth(s, size);
			var px:Number = Math.round(x);
			var py:Number = Math.round(y);
			switch (align) {
				case ALIGN_RIGHT:
					px -= drawWidth;
					break;
				case ALIGN_CENTER:
					px -= drawWidth * 0.5;
					break;
			}
			
			var scale:Number = size / characterHeight;
			var scaleS:Number = scale*0.8;
			for (var i:int = 0; i < s.length; i++ ) {
				var c:int = s.charCodeAt(i);
				if (c == 32) {
					px += widths[32] * scale;
				} else if (c >= 33 && c <= 96) {
					if (font[c] != null) {
						font[c].drawScaled(graphics, px, py, scale, scale);
						px += widths[c] * scale + characterSpacing * scale * characterHeight;
						px = Math.round(px);
					}
				} else if (c >= 97 && c <= 97 + 25) {
					if (smallCaps) {
						c -= 32;
						if (font[c] != null) {
							font[c].drawScaled(graphics, px, py, scaleS, scaleS);
							px += widths[c] * scale * 0.8 + characterSpacing * scale * characterHeight;
						}
					} else {
						if (font[c] != null) {
							font[c].drawScaled(graphics, px, py, scale, scale);
							px += widths[c] * scale + characterSpacing * scale * characterHeight;
						}
					}
				}
			}
			return drawWidth;
		}
		
		public static function drawText(s:String, graphics:Graphics, x:Number, y:Number , size:Number, align:String):Point {
			var scale:Number = size / characterHeight;			
			var r:Point = new Point(0, 0);
			if (!s) return r;
			while (s.length>0) {
				r.y += scale * lineHeight * characterHeight;
				var p:int = s.indexOf("|");
				var l:String;
				if (p >= 0) {
					l = s.substr(0, p);
					s = s.substr(p + 1);
				} else {
					l = s;
					s = "";
				}
				var w:Number = drawTextLine(l, graphics, x, y, size, align);
				if (w > r.x) r.x = w;
				y += scale * lineHeight * characterHeight;
			}
			return r;
		}
		
		public static function drawTextLineToSVG(s:String, group:XML, x:Number, y:Number, size:Number, align:String, fill:String):void {
			var textSvg:XML = <text/>;
			textSvg.@x = x.toFixed(2);
			textSvg.@y = y.toFixed(2);
			size *= 1.5;
			var anchor:String = "start";
			if (align == "center") anchor = "middle";
			if (align == "right") anchor = "end";
			textSvg.@style = "font-size:" + size.toFixed(0) + "px;font-style:normal;font-weight:normal;text-align:" + align + ";text-anchor:" + anchor + ";fill:" + fill + ";fill-opacity:1;stroke:none;stroke-width:1px;stroke-linecap:butt;stroke-linejoin:miter;stroke-opacity:1;font-family:Bitstream Vera Sans";
			
			while (true) {
				var p:int = s.indexOf("|");
				if (p < 0) break;
				s = s.substr(0, p) + String.fromCharCode(27) + s.substr(p + 1);
			}
			textSvg.appendChild(s);
			
			group.appendChild(textSvg);			
			
		}
		
		public static function drawTextToSVG(s:String, svg:XML, x:Number, y:Number, size:Number, align:String, fill:String, stroke:String, strokeWidth:Number):void {
			var scale:Number = size / characterHeight;	
			var group:XML = <g/>;			
			
			if (!s) return;
			while (s.length>0) {
				var p:int = s.indexOf("|");
				var l:String;
				if (p >= 0) {
					l = s.substr(0, p);
					s = s.substr(p + 1);
				} else {
					l = s;
					s = "";
				}
				drawTextLineToSVG(l, group, x, y, size, align, stroke);
				y += scale * lineHeight * characterHeight;
			}
			
			svg.appendChild(group);
			
		}		
		
	}

