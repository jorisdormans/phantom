		public static function textWidth(s:String, size:Number):Number {
			var scale:Number = size / characterHeight;
			var w:Number = 0;
			var x:Number = 0;
			for (var i:int = 0; i < s.length; i++ ) {
				var c:int = s.charCodeAt(i);
				if (c == 32) {
					x += widths[32] * scale;
				} else if (c == 124) {
					x = 0;
				} else if (c >= 33 && c <= 96) {
					if (font[c] != null) {
						x += widths[c] * scale + characterSpacing * scale * characterHeight;
						x = Math.round(x);
					}
				} else if (c >= 97 && c <= 97 + 25) {
					if (smallCaps) {
						c -= 32;
						if (font[c] != null) {
							x += widths[c] * scale * 0.8 + characterSpacing * scale * characterHeight;
						}
					} else {
						if (font[c] != null) {
							x += widths[c] * scale + characterSpacing * scale * characterHeight;
						}
					}
				}
				if (x > w) w = x;
			}
			return w-scale*characterSpacing-scale*widths[32];
		}
		
